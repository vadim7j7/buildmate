# React Native / Expo Environment Configuration
# Copy this file to .env and fill in the values

# API
EXPO_PUBLIC_API_URL=http://localhost:4000

# Authentication
EXPO_PUBLIC_AUTH_PROVIDER=expo

# App Configuration
EXPO_PUBLIC_APP_NAME=MyApp
EXPO_PUBLIC_APP_SCHEME=myapp

# Sentry (Error Tracking)
EXPO_PUBLIC_SENTRY_DSN=
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

# Analytics
EXPO_PUBLIC_POSTHOG_API_KEY=
EXPO_PUBLIC_POSTHOG_HOST=

# Push Notifications
EXPO_PUBLIC_ONESIGNAL_APP_ID=
# or for FCM
GOOGLE_SERVICES_JSON=

# Maps
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=

# Feature Flags
EXPO_PUBLIC_FEATURE_NEW_ONBOARDING=false

# EAS Build
EXPO_TOKEN=

# iOS Specific
APPLE_TEAM_ID=
APPLE_APP_STORE_CONNECT_API_KEY_ID=
APPLE_APP_STORE_CONNECT_ISSUER_ID=

# Android Specific
ANDROID_KEYSTORE_PASSWORD=
ANDROID_KEY_ALIAS=
ANDROID_KEY_PASSWORD=

# Development
LOCAL_IP=192.168.1.100
